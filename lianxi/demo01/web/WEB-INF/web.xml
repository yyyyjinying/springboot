<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
	<!--Session Configauration-->
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
 	<!--配置Servlet  -->
 		<!--配置servlet类路径  -->
 		<servlet>
 			<servlet-name>my</servlet-name>
 			<servlet-class>com.bjsxt.servlet.MyServlet</servlet-class>
			<load-on-startup>1</load-on-startup>
 		</servlet>
 		<!--配置访问方式 21 -->
 		<servlet-mapping>
 			<servlet-name>my</servlet-name>
 			<url-pattern>/my</url-pattern>
 		</servlet-mapping>
 	<!--配置第二个servlet  -->

 		<servlet>
 			<servlet-name>my2</servlet-name>
 			<servlet-class>com.bjsxt.servlet.MyServlet2</servlet-class>
			<load-on-startup>2</load-on-startup>
 		</servlet>
 		<servlet-mapping>
 			<servlet-name>my2</servlet-name>
 			<url-pattern>/my2</url-pattern>
 		</servlet-mapping>

	<servlet>
		<servlet-name>method</servlet-name>
		<servlet-class>com.bjsxt.servlet.ServletMethod</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>method</servlet-name>
		<url-pattern>/method</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>m3</servlet-name>
		<servlet-class>com.bjsxt.servlet.RequestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>m3</servlet-name>
		<url-pattern>/m3</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>m4</servlet-name>
		<servlet-class>com.bjsxt.servlet.ResponseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>m4</servlet-name>
		<url-pattern>/m4</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>page</servlet-name>
		<servlet-class>com.bjsxt.page.PageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>page</servlet-name>
		<url-pattern>/page</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.bjsxt.page.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>main</servlet-name>
		<servlet-class>com.bjsxt.page.MainServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>main</servlet-name>
		<url-pattern>/login/main</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>session01</servlet-name>
		<servlet-class>com.session.Demo01Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>session01</servlet-name>
		<url-pattern>/session/session01</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>session02</servlet-name>
		<servlet-class>com.session.Demo02Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>session02</servlet-name>
		<url-pattern>/session/session02</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>el01</servlet-name>
		<servlet-class>com.el.servlet.ElServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>el01</servlet-name>
		<url-pattern>/el01</url-pattern>
	</servlet-mapping>
</web-app>
